<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<app-component xmlns="http://schemas.haulmont.com/cuba/app-component.xsd"
               dependsOn="com.haulmont.cuba, com.haulmont.addon.search">
    <module blocks="*"
            name="global">
        <artifact appJar="true"
                  name="instantlauncher-global"/>
        <property name="cuba.persistenceConfig"
                  value="+de/diedavids/cuba/instantlauncher/persistence.xml"/>
        <property name="cuba.metadataConfig"
                  value="+de/diedavids/cuba/instantlauncher/metadata.xml"/>
        <property name="cuba.viewsConfig"
                  value="+de/diedavids/cuba/instantlauncher/views.xml"/>
    </module>
    <module blocks="core"
            dependsOn="global"
            name="core">
        <artifact appJar="true"
                  name="instantlauncher-core"/>
        <artifact classifier="db"
                  configuration="dbscripts"
                  ext="zip"
                  name="instantlauncher-core"/>
        <property name="cuba.springContextConfig"
                  value="+de/diedavids/cuba/instantlauncher/spring.xml"/>
        <property name="cuba.mainMessagePack"
                  value="+de.diedavids.cuba.instantlauncher.core"/>
    </module>
    <module blocks="web"
            dependsOn="global"
            name="web">
        <artifact appJar="true"
                  name="instantlauncher-web"/>
        <artifact classifier="web"
                  configuration="webcontent"
                  ext="zip"
                  name="instantlauncher-web"/>
        <property name="cuba.springContextConfig"
                  value="+de/diedavids/cuba/instantlauncher/web-spring.xml"/>
        <property name="cuba.dispatcherSpringContextConfig"
                  value="+de/diedavids/cuba/instantlauncher/web-dispatcher-spring.xml"/>
        <property name="cuba.windowConfig"
                  value="+de/diedavids/cuba/instantlauncher/web-screens.xml"/>
        <property name="cuba.menuConfig"
                  value="+de/diedavids/cuba/instantlauncher/web-menu.xml"/>
        <property name="cuba.permissionConfig"
                  value="+de/diedavids/cuba/instantlauncher/web-permissions.xml"/>
        <property name="cuba.mainMessagePack"
                  value="+de.diedavids.cuba.instantlauncher.web"/>
    </module>
</app-component>
